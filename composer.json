{
    "name": "redde/php-api-sdk",
    "description": "Redde API wrapper",
    "type": "library",
    "keywords": ["online payment","payment integration","wigal","wigal solutions", "mobile money", "card payments"],
    "homepage": "https://www.reddeonline.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Oteng Kwame Appiah-Nti",
            "email": "koteng@wigal.com.gh",
            "role": "Software Engineer"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
